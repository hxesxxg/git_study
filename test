public class KeyVaultController {
  public static void main(String[] args) {
    String keyVaultUrl = "https://kv-kr02-krc-d-plus-vlt01.vault.azure.net";

    CertificateClient certificateClient = new CertificateClientBuilder()
      .vaultUrl(keyVaultUrl)
      .credential(new DefaultAzureCredentialBuilder().build())
      .buildClient();

    for (CertificateProperties certificateProperties : certificateClient.listPropertiesOfCertificates()) {
        KeyVaultCertificate certificateWithAllProperties =
            certificateClient.getCertificateVersion(certificateProperties.getName(), certificateProperties.getVersion());
        System.out.printf("Received certificate with name \"%s\" and secret id %s",
            certificateWithAllProperties.getProperties().getName(), certificateWithAllProperties.getSecretId());
    }
    String certificateName = "AIAPLUSBIZ-DEV-API-CERTIFICATE";

    CertificateProperties certificateProperties = certificateClient.getCertificate(certificateName).getProperties();
    log.debug("Certificate Name: " + certificateProperties.getName());
    log.debug("Certificate Id: " + certificateProperties.getId());
//	System.out.println("Certificate Attributes: " + certificateProperties.getAttributes());
	log.debug("Certificate Tags: " + certificateProperties.getTags());
    log.debug("Certificate x5t: " + certificateProperties.getX509Thumbprint());
	log.debug("Certificate x5tas: " + certificateProperties.getX509ThumbprintAsString());
	log.debug("Certificate RecoverableDays: " + certificateProperties.getRecoverableDays());
	log.debug("Certificate Version: " + certificateProperties.getVersion());
  }
}
